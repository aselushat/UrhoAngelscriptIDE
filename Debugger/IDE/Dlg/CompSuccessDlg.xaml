<mui:ModernDialog x:Class="Debugger.IDE.Dlg.CompSuccessDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Compile Complete" Height="300" Width="300">
    <mui:ModernDialog.Resources>
        <SolidColorBrush Color="Green" x:Key="Accent"/>
        <SolidColorBrush Color="Green" x:Key="BorderBrush" />
        <SolidColorBrush Color="Green" x:Key="WindowBorderActive" />
        <SolidColorBrush Color="Green" x:Key="WindowBorderBackground" />
    </mui:ModernDialog.Resources>
    <Grid>
        <TextBox Text="Compilation Successful" />
    </Grid>
</mui:ModernDialog>
